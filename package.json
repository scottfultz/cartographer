{
  "name": "@cf/cartographer-monorepo",
  "version": "0.0.0",
  "description": "Cartographer Engine, Atlas SDK, and Suite of Local-First SEO/Accessibility/Performance Tools",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/scottfultz/cartographer.git"
  },
  "author": "Cai Frazier",
  "license": "UNLICENSED",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:cov": "turbo run test -- --coverage",
    "typecheck": "tsc -b",
    "clean": "turbo run clean && rimraf node_modules/.cache",
    "demo:quickstart": "cd packages/cartographer && node scripts/demo-quickstart.js",
    "release": "changeset version && pnpm install --no-frozen-lockfile && turbo run build && changeset publish",
    "prepare": "husky install || true"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "husky": "^9.0.0",
    "prettier": "^3.3.0",
    "rimraf": "^6.0.1",
    "turbo": "^2.0.0",
    "typescript": "^5.6.3",
    "vitest": "^2.0.0"
  }
}
