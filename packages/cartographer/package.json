{
  "name": "@cf/cartographer",
  "version": "1.0.0",
  "description": "Production-grade headless web crawler producing Atlas v1.0 (.atls) archives",
  "type": "module",
  "author": "Cai Frazier",
  "license": "UNLICENSED",
  "private": true,
  "keywords": [
    "crawler",
    "web-crawler",
    "headless",
    "playwright",
    "seo",
    "accessibility",
    "atlas"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scottfultz/cartographer.git",
    "directory": "packages/cartographer"
  },
  "bugs": {
    "url": "https://github.com/scottfultz/cartographer/issues"
  },
  "homepage": "https://github.com/scottfultz/cartographer#readme",
  "bin": {
    "cartographer": "./dist/cli/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "clean": "rimraf dist dist-tests",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@atlas/sdk": "workspace:*",
    "@atlas/spec": "workspace:*",
    "@cf/url-tools": "workspace:*",
    "@fast-csv/format": "^5.0.0",
    "@mongodb-js/zstd": "^1.2.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "archiver": "^7.0.1",
    "cheerio": "^1.1.2",
    "fast-glob": "^3.3.2",
    "minimatch": "^10.0.3",
    "p-limit": "^5.0.0",
    "picocolors": "^1.0.0",
    "playwright": "^1.48.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "simple-wappalyzer": "^1.1.83",
    "undici": "^6.19.8",
    "yargs": "^17.7.2",
    "yauzl": "^3.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.14.10",
    "@types/yauzl": "^2.10.3",
    "@types/yargs": "^17.0.33",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^2.0.0"
  }
}
