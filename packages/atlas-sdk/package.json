{
  "name": "@atlas/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for reading Atlas v1.0 archives produced by Cartographer Engine",
  "author": "Cai Frazier",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "test": "vitest run",
    "lint": "eslint .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@atlas/spec": "workspace:*",
    "@mongodb-js/zstd": "^1.2.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "yauzl": "^3.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/yauzl": "^2.10.3",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "atlas",
    "crawler",
    "seo",
    "accessibility",
    "sdk"
  ]
}
