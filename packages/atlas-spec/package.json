{
  "name": "@atlas/spec",
  "version": "1.0.0-beta.1",
  "description": "Atlas v1.0 specification, JSON Schemas, TypeScript types, and validators",
  "type": "module",
  "author": "Cai Frazier",
  "license": "UNLICENSED",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b && node dist/schemas/generator.js",
    "build:schemas": "node dist/schemas/generator.js",
    "dev": "tsc -b --watch",
    "test": "vitest run",
    "lint": "eslint .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "typescript": "^5.6.3",
    "vitest": "^2.0.0"
  }
}
